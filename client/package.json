{
    "name": "ringofsnakes-client",
    "version": "0.10.2",
    "description": "",
    "repository": {
        "type": "git",
        "url": "https://github.com/KielerGames/ringofsnakes.git"
    },
    "private": true,
    "scripts": {
        "test": "jest --maxWorkers=1",
        "pack:shaders:dev": "node pack-shaders.js",
        "pack:shaders": "node pack-shaders.js --minify",
        "build:js": "webpack --progress",
        "build:js:watch": "webpack --watch",
        "build": "npm run pack:shaders:dev && npm run build:js",
        "build:production": "npm run pack:shaders && webpack --mode=production",
        "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\"",
        "lint": "eslint ./src/app --ext .ts,.tsx",
        "check:format": "prettier --check --no-color \"src/**/*.ts\" \"src/**/*.tsx\"",
        "coverage": "jest --maxWorkers=1 --coverage"
    },
    "author": "KielerGames",
    "devDependencies": {
        "@types/jest": "^27.4.1",
        "@typescript-eslint/eslint-plugin": "^5.37.0",
        "@typescript-eslint/parser": "^5.37.0",
        "css-loader": "^6.7.1",
        "eslint": "^8.23.1",
        "html-webpack-plugin": "^5.5.0",
        "jest": "^27.5.1",
        "less": "^4.1.3",
        "less-loader": "^11.0.0",
        "prettier": "^2.7.1",
        "rand-seed": "^1.0.2",
        "style-loader": "^3.3.1",
        "ts-jest": "^27.1.3",
        "ts-loader": "^9.4.1",
        "typescript": "^4.8.3",
        "webpack": "^5.74.0",
        "webpack-bundle-analyzer": "^4.6.1",
        "webpack-cli": "^4.10.0"
    },
    "dependencies": {
        "comlink": "^4.3.1",
        "preact": "^10.11.0"
    },
    "engines": {
        "node": "~16",
        "npm": "~8"
    }
}
